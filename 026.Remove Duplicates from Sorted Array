class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        if (nums.size() == 0)
            return 0;
        
        int count = 1;
        int currentNumber = nums[0];
        
        for (int i=1; i<nums.size(); i++) {
            if (nums[i] != currentNumber) {
                currentNumber = nums[i];
                nums[count] = currentNumber;
                count ++;
            }
        }
        
        return count;
    }
};
