class Solution {
public:
    vector<int> getRow(int rowIndex) {
        vector<int> result;
        result.push_back(1);
        for (int i=0; i < rowIndex; i++) {
            result.insert(result.begin(), 1);
            for (int j=1; j<i+1; j++) {
                result[j] += result[j+1];
            }
        }
        
        return result;
    }
};
