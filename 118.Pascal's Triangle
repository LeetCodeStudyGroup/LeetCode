class Solution {
public:
    vector<vector<int>> generate(int numRows) {
        vector<vector<int>> result;
        vector<int> row1;
        vector<int> row2;
        for (int i=0; i < numRows; i++) {
            for (int j=0; j < row1.size()+1; j++) {
                if (row2.size() == 0 || row1.size() == j) {
                    row2.push_back(1);
                } else {
                    row2.push_back(row1[j-1] + row1[j]);
                }
            }
            row1 = row2;
            result.push_back(row2);
            row2.clear();
        }
        return result;
    }
};
